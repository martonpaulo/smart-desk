{
  "expo": {
    "name": "Smart Desk",
    "slug": "smart-desk",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6200ee"
    },
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartdesk.app"
    },
    "plugins": ["expo-font"],
    "scheme": "smartdesk"
  }
}
